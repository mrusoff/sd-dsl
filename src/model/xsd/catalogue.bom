package catalogue prefix cat namespace "org.toto" {
	
	datatype String
	datatype Date
	
	entity Catalogue {
		name : String 
		ref  publications : Publication * 
	} 
	
	entity Publication {
		title : String
		author : String *
		date :  Date
	} 
	
	entity BookType extends Publication {
		isbn : String
		publisher : String
	}
	
	entity Magazine extends Publication {
	}
	
}	