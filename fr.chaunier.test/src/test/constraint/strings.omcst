package test.constraint.strings prefix dat namespace "http://www.chaunier.fr/date" {


	datatype Name 

	entity MyStrings1 ''{
		defaultStr : Name 							"string par defaut"
		str : Name string 					"definition type string"
		str20 : Name string minLen 2			"definition min len 2"
		str02 : Name string maxLen 2			"definition max len 2"
		str25 : Name string minLen 2 maxLen 5	"definition min len 2 et max len 5"
		str22 : Name string fixeLen 2 			"definition fixe len 2"
		strFixe : Name string fixed "ff"			"definition fixed value ff"
		strDef : Name string default "DEFAUT"	"definition default DEFAUT"
	}

	element MyStrings1

		datatype DT 							"string par defaut"
		datatype DTS string 					"definition type string"
		datatype DTS20 string minLen 2			"definition min len 2"
		datatype DTS02 string maxLen 2			"definition max len 2"
		datatype DTS25 string minLen 2 maxLen 5	"definition min len 2 et max len 5"
		datatype DTS22 string fixeLen 2 			"definition fixe len 2"
		datatype DTSFIXE string fixed "ff"			"definition fixed value ff"
		datatype DTSDF string default "DEFAUT"	"definition default DEFAUT"

	element MyStrings2


	entity MyStrings2 ''{
		defaultStr : DT 		"string par defaut"
		str : DTS 				"definition type string"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	entity MyStrings3 ''{
		defaultStr : DT string		"string par defaut"
		str : DTS string maxLen 5	"definition type string overide max len 5"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	element MyStrings3

	entity MyStrings4 ''{
		defaultStr : DT decimal		"surcharge type"
//		str : DTS boolean 		"definition type string"			// ERROR
//		str20 : DTS20 string minLen 5	"definition min len 2"		// ERROR
		str02 : DTS02 string minLen 1	"definition max len 2 overide min len 1"		// warning surcharge minLen sur def maxLen
		str25 : DTS25 string fixed "ffff"	"definition min len 2 et max len 5 overide fixed fffff"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE 		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		


	element MyStrings4
}

