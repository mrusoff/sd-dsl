package test.constraint.integers prefix dat namespace "http://www.chaunier.fr/date" {

	datatype Name 

	entity MyIntegers ''{
		
		c : Name integer
		c4 : Name integer fixeLen 2 
		c55 : Name integer maxVal 2 
		c6 : Name integer minVal 2 
		c7 : Name integer maxVal 2
		c8 : Name integer minVal 2 
		d3 : Name integer default 0 //paddle "NNN.NN"
	}

	element MyIntegers	

		datatype DT 							"string par defaut"
		datatype DTS integer 					"definition type string"
		datatype DTS20 integer maxVal 2			"definition min len 2"
		datatype DTS02 integer maxVal 2			"definition max len 2"
		datatype DTS25 integer minVal 2 maxVal 5	"definition min len 2 et max len 5"
		datatype DTS22 integer fixeLen 2 			"definition fixe len 2"
		datatype DTSFIXE integer fixed 2			"definition fixed value ff"
		datatype DTSDF integer default 00	"definition default DEFAUT"

	element MyIntegers2

	entity MyIntegers2 ''{
		defaultStr : DT 		"string par defaut"
		str : DTS 				"definition type string"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	entity MyIntegers3 ''{
		defaultStr : DT 		"string par defaut"
		str : DTS integer	"definition type string"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	element MyIntegers3


	entity MyIntegers4 ''{
		defaultStr : DT integer		"surcharge type"
//		str : DTS boolean 		"definition type string"			// ERROR
//		str20 : DTS20 integer minLen 5	"definition min len 2"		// ERROR
		str02 : DTS02 integer minVal 1	"definition max len 2"		// warning surcharge minLen sur def maxLen
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	element MyIntegers4


}

