package test.constraint.decimals prefix dat namespace "http://www.chaunier.fr/date" {

	datatype Name 

	entity MyDecimals ''{
		
		c : Name decimal
		c1 : Name decimal precision 2
		c2 : Name decimal precision 2
		c3 : Name decimal precision 5 scale 2
		c4 : Name decimal fixeLen 2 
		c55 : Name integer maxVal 2 
		c6 : Name integer minVal 2 
		c7 : Name decimal maxVal 2.2 
		c8 : Name decimal minVal 2.2 
		d3 : Name decimal default 0.0 //paddle "NNN.NN"
	}

	element MyDecimals	

		datatype DT 							"string par defaut"
		datatype DTS decimal 					"definition type string"
		datatype DTS20 decimal maxVal 2.0			"definition min len 2"
		datatype DTS02 decimal maxVal 2.0			"definition max len 2"
		datatype DTS25 decimal minVal 2.0 maxVal 5.10	"definition min len 2 et max len 5"
		datatype DTS22 decimal fixeLen 2 			"definition fixe len 2"
		datatype DTSFIXE decimal fixed 0.2			"definition fixed value ff"
		datatype DTSDF decimal default 0.00	"definition default DEFAUT"

	element MyDecimals2

	entity MyDecimals2 ''{
		defaultStr : DT 		"string par defaut"
		str : DTS 				"definition type string"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	entity MyDecimals3 ''{
		defaultStr : DT 		"string par defaut"
		str : DTS decimal	"definition type string"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	element MyDecimals3


	entity MyDecimals4 ''{
		defaultStr : DT decimal		"surcharge type"
//		str : DTS boolean 		"definition type string"			// ERROR
//		str20 : DTS20 decimal minLen 5	"definition min len 2"		// ERROR
		str02 : DTS02 decimal minVal 1.0	"definition max len 2"		// warning surcharge minLen sur def maxLen
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	element MyDecimals4


}

