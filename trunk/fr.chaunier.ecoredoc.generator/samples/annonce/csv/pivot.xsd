	<xsd:complexType name="VATType">
        <xsd:annotation>
        	<xsd:documentation>VAT definition</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="code">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="0"></xsd:enumeration>
						<xsd:enumeration value="1"></xsd:enumeration>
						<xsd:enumeration value="2"></xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="percentage" type="xsd:string"></xsd:element>
			<xsd:element name="amount" type="pvt:AmountType"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="AmountType">
		<xsd:annotation>
			<xsd:documentation>
				fr : expression d'une valeure monaitaire.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="value" type="xsd:int">
			<xsd:annotation>
				<xsd:documentation>
					un montant est exprimé en 100 éme d'unité monaitaire
					(currency)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="currency" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>iso code of currency EUR pour euro</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<!--xsd:sequence>
			<xsd:element name="value" type="xsd:int">
			<xsd:annotation>
			<xsd:documentation>
			un montant en euro est exprimé en 100 éme
			d'unité monaitaire (currency)
			</xsd:documentation>
			</xsd:annotation>
			</xsd:element>
			<xsd:element name="currency" type="xsd:string" maxOccurs="1" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>iso code of currency</xsd:documentation>
			</xsd:annotation></xsd:element>
			</xsd:sequence-->
	</xsd:complexType>


	<xsd:simpleType name="ParcelNumber">
        <xsd:annotation>
        	<xsd:documentation>parcel id (sequence number)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:positiveInteger">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ParcelCAB">
        <xsd:annotation>
        	<xsd:documentation>parcel CAB : service code + sequence number + checksum </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="lineType35">
        <xsd:annotation>
        	<xsd:documentation>string type 35 char limited</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
			<xsd:minLength value="35"></xsd:minLength>
		</xsd:restriction>
	</xsd:simpleType>

