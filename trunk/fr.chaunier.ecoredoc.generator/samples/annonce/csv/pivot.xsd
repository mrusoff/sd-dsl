    <xsd:complexType name="WeightType">
        <xsd:annotation>
        	<xsd:documentation>weight informations
</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
    		<xsd:element name="taxable" type="xsd:float">
    			<xsd:annotation>
    				<xsd:documentation>taxable weight fr : poid taxable</xsd:documentation>
    			</xsd:annotation></xsd:element>
    		<xsd:element name="rounded" type="xsd:float">
    			<xsd:annotation>
    				<xsd:documentation>rouded weight fr : poid arroudi</xsd:documentation>
    			</xsd:annotation></xsd:element>
    		<xsd:element name="applied" type="xsd:float">
    			<xsd:annotation>
    				<xsd:documentation>applied weight fr : poid appliqué </xsd:documentation>
    			</xsd:annotation></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="ExtraWeightOrVolumeEnum">
    	<xsd:restriction base="xsd:string">
    		<xsd:enumeration value="N"></xsd:enumeration>
    		<xsd:enumeration value="D"></xsd:enumeration>
    		<xsd:enumeration value="S"></xsd:enumeration>
    		<xsd:enumeration value="E"></xsd:enumeration>
    	</xsd:restriction>
    </xsd:simpleType>




    <xsd:complexType name="PALERTType">
    	<xsd:sequence>
    		<xsd:element name="consignor" type="pvt:AddressType">
    			<xsd:annotation>
    				<xsd:documentation>
    					Expéditeur du colis
    				</xsd:documentation>
    			</xsd:annotation>
    		</xsd:element>
    		<xsd:element name="consignee" type="pvt:AddressType">
    			<xsd:annotation>
    				<xsd:documentation>
    					Destinataire du colis
    				</xsd:documentation>
    			</xsd:annotation>
    		</xsd:element>
    		<xsd:element name="cashOnDelivery"
    			type="pvt:CashOnDeliveryType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                	<xsd:documentation>cash on delivery  fr : Paiement à la livraison</xsd:documentation>
                </xsd:annotation>
    		</xsd:element>
    		<xsd:element name="transportation" type="pvt:AmountType">
    			<xsd:annotation>
    				<xsd:documentation>montant lié au transport</xsd:documentation>
    			</xsd:annotation></xsd:element>
    		<xsd:element name="insurance" type="pvt:InsuranceType" maxOccurs="1" minOccurs="0">
    			<xsd:annotation>
    				<xsd:documentation>shipping Insurance fr : assurance</xsd:documentation></xsd:annotation></xsd:element>
    		<xsd:element name="certification"
    			type="pvt:CertificationType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                	<xsd:documentation>certification , fr : niveau de certification syn : niveau recommandation </xsd:documentation></xsd:annotation>
    		</xsd:element>
            <xsd:element name="extraWeightOrVolume" type="pvt:ExtraWeightOrVolumeType" maxOccurs="1" minOccurs="0">
            	<xsd:annotation>
            		<xsd:documentation>Extra weight or volume amount : fr : montant supplémnt hors gabarit , non mécanisable, traitement manuel
	</xsd:documentation>
            	</xsd:annotation></xsd:element>
            <xsd:element name="islandExtraFee"
    			type="pvt:AmountType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                	<xsd:documentation>island extr feee</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="acknowledgment" type="pvt:AmountType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                	<xsd:documentation>acknowledgment fr : avis de réception</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="returnOption" type="pvt:AmountType" maxOccurs="1" minOccurs="0">
            	<xsd:annotation>
            		<xsd:documentation>return option fr : option de retour</xsd:documentation>
            	</xsd:annotation></xsd:element>
            <xsd:element name="saturdayDelivery"
    			type="pvt:AmountType" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                	<xsd:documentation>saturday delivery fr option de livraison samedi</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
    		<xsd:element name="VAT" type="pvt:VATType">
    			<xsd:annotation>
    				<xsd:documentation>VAT</xsd:documentation>
    			</xsd:annotation></xsd:element>
            <xsd:element name="totalAmount" type="pvt:AmountType">
    			<xsd:annotation>
    				<xsd:documentation>montant total </xsd:documentation>
    			</xsd:annotation></xsd:element>
            <xsd:element name="freeDutyAndTax" type="pvt:AmountType" maxOccurs="1" minOccurs="0"></xsd:element>
    		<xsd:element name="weight" type="pvt:WeightType"></xsd:element>
    	</xsd:sequence>
    	<xsd:attribute name="parcelID" type="xsd:string">
    		<xsd:annotation>
    			<xsd:documentation>CAB du colis</xsd:documentation>
    		</xsd:annotation>
    	</xsd:attribute>
    	<xsd:attribute name="masterParcelID" type="xsd:string"></xsd:attribute>
    	<xsd:attribute name="operatorCode" type="xsd:string">
    		<xsd:annotation>
    			<xsd:documentation>code operateur</xsd:documentation>
    		</xsd:annotation>
