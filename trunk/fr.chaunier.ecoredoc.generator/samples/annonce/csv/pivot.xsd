	<xsd:complexType name="AddressType">
		<xsd:annotation>
			<xsd:documentation>
				adress definition, définition standard d'une adresse
				pour coliposte et chronopost
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name1" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>raison social ou nom prénom</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<xsd:attribute name="name2" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>is raison social dans name1 alors nom prénom</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<xsd:attribute name="line1" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>premiere ligne d'adresse</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<xsd:attribute name="line2" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>seconde ligne d'adresse</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<xsd:attribute name="city" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>ville</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<xsd:attribute name="zipCode" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>code postal</xsd:documentation>
			</xsd:annotation></xsd:attribute>
		<xsd:attribute name="state" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation></xsd:documentation>
			</xsd:annotation></xsd:attribute>
        <xsd:attribute name="countryCode" type="xsd:string">
        	<xsd:annotation>
        		<xsd:documentation>code iso du pays</xsd:documentation>
        	</xsd:annotation></xsd:attribute>
        <xsd:attribute name="phone" type="xsd:string"></xsd:attribute>
		<xsd:attribute name="email" type="xsd:string"></xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="InsuranceType">
		<xsd:annotation>
			<xsd:documentation>shipping Insurance fr : assurance</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="tax" type="pvt:AmountType"></xsd:element>
			<xsd:element name="value" type="pvt:AmountType"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CertificationType">
		<xsd:annotation>
			<xsd:documentation>certification , fr : niveau de certification syn : niveau recommandation </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="label" type="xsd:string"></xsd:element>
			<xsd:element name="tax" type="pvt:AmountType"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
