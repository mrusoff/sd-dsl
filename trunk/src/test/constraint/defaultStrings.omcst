package test.constraint.defaults prefix dat namespace "http://www.chaunier.fr/date" {

	datatype Name 

		datatype DT 							"string par defaut"
		datatype DTS string 					"definition type string"
		datatype DTS20 string minLen 2			"definition min len 2"
		datatype DTS02 string maxLen 2			"definition max len 2"
		datatype DTS25 string minLen 2 maxLen 5	"definition min len 2 et max len 5"
		datatype DTS22 string fixeLen 2 			"definition fixe len 2"
		datatype DTSFIXE string fixed "ff"			"definition fixed value ff"
		datatype DTSDF string default "DEFAUT"	"definition default DEFAUT"


	entity MyStrings2 ''{
		defaultStr : DT 		"string par defaut"
		str : DTS 				"definition type string"
		str20 : DTS20			"definition min len 2"
		str02 : DTS02			"definition max len 2"
		str25 : DTS25			"definition min len 2 et max len 5"
		str22 : DTS22 			"definition fixe len 2"
		strFixe : DTSFIXE		"definition fixed value ff"
		strDef : DTSDF			"definition default DEFAUT"
	}		

	element MyStrings2
}

