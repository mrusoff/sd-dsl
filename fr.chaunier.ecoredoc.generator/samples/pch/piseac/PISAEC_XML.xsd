	<xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="root">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="enregistrement_hhh" minOccurs="1" maxOccurs="1"/>
